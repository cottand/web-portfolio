{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "typeRoots": [
      "src/types",
      "src/types/imported",
      "node_modules/@types"
    ],
    "jsx": "react-jsx",
    "types": [
//      "vite/client",
//      "vite-plugin-svgr/client"
    ]
  },
  "include": [
    "src/*"
    //    "src/components",
  ]
}
